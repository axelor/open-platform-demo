<?xml version="1.0" encoding="UTF-8"?>
<domain-models xmlns="http://apps.axelor.com/xml/ns/domain-models"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://apps.axelor.com/xml/ns/domain-models http://apps.axelor.com/xml/ns/domain-models/domain-models_0.6.xsd">

  <module name="contact" package="com.axelor.contact.db"/>

  <entity name="Company">
    <string name="code" required="true" unique="true" min="2"/>
    <string name="name" required="true" unique="true" min="2"/>
    <many-to-one name="parent" ref="Company"/>
    <one-to-many name="employees" ref="Contact"/>
    <many-to-many name="circles" ref="Circle"/>
    <string name="notes" multiline="true"/>
  </entity>

</domain-models>
